{
	"symbols":{
		"query_operator": {
			"terminal": true,
			"match": "\\?="
		},
		"implies": {
			"terminal":true,
			"match":":-",
			"excludeFromProduction":true
		},
		"fact_list": {
			"terminal": false,
			"mergeRecursive": true
		},
		"argument_list": {
			"terminal": false,
			"mergeRecursive": true
		},
		"argument": {
			"terminal":false,
			"mergeIntoParent": true
		},
		"concatenation":{
			"terminal": false,
			"mergeRecursive":true
		},
		"Q.statement": {
			"terminal": false
		}
	},
	"modules":{
		"Q":"./grammars/query.json"
	},
	"productions": {
		"literal": [
			[ "Q.numeric-literal" ],
			[ "Q.string-literal" ],
			[ "Q.id" ]
		],
		"statement_list": [
			["statement_list", "statement"],
			["statement"]
		],
		"query": [
			["query_operator", "Q.statement"]
		],
		"statement": [
			["infix", "Q.dot"],
			["fact", "Q.dot"],
			["rule", "Q.dot"],
			["query"]
		],
		"infix": [
			["Q.variable_name", "Q.infix_operator", "Q.variable_name"],
			["Q.variable_name", "Q.infix_operator", "literal"],
			["Q.variable_name", "Q.infix_operator", "fact"],
			["Q.variable_name", "Q.infix_operator", "list"],
			["literal", "Q.infix_operator", "Q.variable_name"],
			["literal", "Q.infix_operator", "literal"],
			["literal", "Q.infix_operator", "fact"],
			["literal", "Q.infix_operator", "list"],
			["fact", "Q.infix_operator", "Q.variable_name"],
			["fact", "Q.infix_operator", "literal"],
			["fact", "Q.infix_operator", "fact"],
			["fact", "Q.infix_operator", "list"],
			["list", "Q.infix_operator", "Q.variable_name"],
			["list", "Q.infix_operator", "literal"],
			["list", "Q.infix_operator", "fact"],
			["list", "Q.infix_operator", "list"]
		],
		"fact": [
			["Q.id", "Q.open_paren", "argument_list", "Q.close_paren"],
			["Q.id", "Q.open_paren", "Q.close_paren"],
			["Q.string-literal", "Q.open_paren", "argument_list", "Q.close_paren"],
			["Q.string-literal", "Q.open_paren", "Q.close_paren"]
		],
		"rule":[
			["fact", "implies", "fact_list"]
		],
		"fact_list": [
			["fact_list", "Q.comma", "fact"],
			["fact"]
		],
		"list": [
			["Q.open_square" ,"Q.close_square"],
			["Q.open_square", "argument_list", "Q.close_square"]
		],
		"argument_list": [
			["argument_list", "Q.comma", "argument"],
			["argument"]
		],
		"argument": [
			["literal"],
			["Q.variable_name"],
			["fact"],
			["list"]
		]
	},
	"startSymbols": [ "statement_list" ]
}
